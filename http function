const https = require('https')
var logindata = JSON.stringify({user: "reef-pi", password: "reef-pi"})

const options = {
  hostname: 'webhook.site',
  path: '/b9416f6a-5dd5-4a16-af99-75f7c8245f53',
  port: 443,
  method: 'POST',
  json: true,
  form: logindata
}

const req = https.request(options)

req.on('error', error => {
  console.error(error)
})

req.end()
         
